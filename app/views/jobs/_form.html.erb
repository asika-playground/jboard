<% if false %>
<% if j.errors.any? %>
      <ul>
      <% j.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
<% end %>
<% end %>

<div class="row form-inline">
    <div class="col-md-6">
        <div class="form-group">
            <%= f.label :title, "Job Title*" %>
            <%= f.text_field :title, :class => "form-control" %>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <%= f.label :company, "Company*" %>
            <%= f.text_field :company, :class => "form-control" %>
        </div>
    </div>

</div>

<div class="row form-inline">
    <div class="col-md-6">
        <div class="form-group">
            <%= f.label :location, "Location*" %>
            <%= f.text_field :location, :class => "form-control" %>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <%= f.label :min_salary, "Min. Salary*" %>
            <%= f.number_field :min_salary, :class => "form-control", :type => "number" %>
        </div>
    </div>
</div>

<div class="row form-inline">
    <div class="col-md-6">
        <div class="form-group">
            <%= f.label :expire, "Expire Date" %>
            <%= f.date_field :expire, :class => "form-control", :type => "date" %>
        </div>
    </div>
    <div class="col-md-6"></div>

</div>


<div class="form-group">
    <%= f.label :url, "Job URL" %>
    <%= f.text_field :url, :class => "form-control", :type => "url" %>
</div>

<div class="form-group">
    <%= f.label :description, "Description" %>
    <%= f.text_area :description, :class => "form-control", :rows => 3 %>
</div>

